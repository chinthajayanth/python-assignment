    "def swap_case(s):\n",
    "    required = \"\"\n",
    "    for i in s:\n",
    "        if (i.isupper()):\n",
    "            required += i.lower()\n",
    "        else:\n",
    "            required += i.upper()\n",
    "    return required\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    s = input()\n",
    "    result = swap_case(s)\n",
    "    print(result)"
   ]
